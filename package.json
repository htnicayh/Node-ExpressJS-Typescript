{
  "name": "typecript",
  "version": "1.0.0",
  "description": "node, typescript",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --exec ts-node src/server.ts",
    "lint": "eslint \"{src, test}/**/*.ts\" --fix",
    "prettier": "prettier --write \"{src, test}/**/*.ts\"",
    "start": "nodemon ./dist/server.js",
    "prod": "npm run build && npm run start",
    "postinstall": "husky install",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/mysql": "^2.15.20",
    "@types/request": "^2.48.8",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "eslint": "^8.7.0",
    "express": "^4.17.2",
    "husky": "^7.0.4",
    "lint-staged": "^12.2.2",
    "npm-run-all": "^4.1.5",
    "request": "^2.88.2",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "tsc": "^2.0.4",
    "tsc-watch": "^4.6.0"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "class-validator": "^0.13.2",
    "cors": "^2.8.5",
    "dotenv": "^14.2.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "reflect-metadata": "^0.1.13",
    "typedi": "^0.8.0",
    "typeorm": "^0.2.41",
    "typeorm-typedi-extensions": "^0.2.3",
    "typescript": "^4.5.5",
    "winston": "^3.4.0",
    "yarn": "^1.22.17"
  },
  "lint-staged": {
    "**/*.ts": [
      "npm run prettier",
      "npm run lint",
      "git add ."
    ]
  }
}
